\n  return (\n    <div className={classes.root}>\n      <Stepper activeStep={activeStep} alternativeLabel>\n        {steps.map((label) => (\n          <Step key={label}>\n            <StepLabel>{label}</StepLabel>\n          </Step>\n        ))}\n      </Stepper>\n      <div>\n        {activeStep === steps.length ? (\n          <div>\n            <Typography className={classes.instructions}>All steps completed</Typography>\n            <Button onClick={handleReset}>Reset</Button>\n          </div>\n        ) : (\n          <div>\n            <Typography className={classes.instructions}>{getStepContent(activeStep)}</Typography>\n            <div>\n              <Button\n                disabled={activeStep === 0}\n                onClick={handleBack}\n                className={classes.backButton}\n              >\n                Back\n              </Button>\n              <Button variant=\"contained\" color=\"primary\" onClick={handleNext}>\n                {activeStep === steps.length - 1 ? 'Finish' : 'Next'}\n              </Button>\n            </div>\n          </div>\n        )}\n      </div>\n    </div>\n  );\n}\n"},hmkQ:function(e,t,n){"use strict";var a=n("z3IF"),o=n("0tNF"),r=n("2Fjn"),i=n("mXGw"),s=n("PDtE"),p=n("XpU0"),l=n("VPks"),c=n("rKPb"),u=n("EseM"),d=i.createElement,m=i.forwardRef((function(e,t){var n=e.activeStep,p=void 0===n?0:n,m=e.backButton,h=e.classes,S=e.className,g=e.LinearProgressProps,b=e.nextButton,v=e.position,f=void 0===v?"bottom":v,y=e.steps,w=e.variant,k=void 0===w?"dots":w,x=Object(r.a)(e,["activeStep","backButton","classes","className","LinearProgressProps","nextButton","position","steps","variant"]);return d(l.a,Object(a.a)({square:!0,elevation:0,className:Object(s.a)(h.root,h["position".concat(Object(c.a)(f))],S),ref:t},x),m,"text"===k&&d(i.Fragment,null,p+1," / ",y),"dots"===k&&d("div",{className:h.dots},Object(o.a)(new Array(y)).map((function(e,t){return d("div",{key:t,className:Object(s.a)(h.dot,t===p&&h.dotActive)})}))),"pro